{
  "name": "ctpad-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "dev": "nest start --watch",
    "prod": "node dist/main",
    "build": "nest build",
    "typeorm-public": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli -d orm-public.config.ts",
    "migrations:public:generate": "npm run typeorm-public migration:generate src/infrastructure/database/migrations/public/$npm_config_name",
    "migrations:public:create": "npm run typeorm-public migration:create src/infrastructure/database/migrations/public/$npm_config_name",
    "migrations:public:run": "npm run typeorm-public migration:run",
    "migrations:public:show": "npm run typeorm-public migration:show",
    "typeorm-tenanted": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli -d orm-tenanted.config.ts",
    "migrations:tenanted:generate": "npm run typeorm-tenanted migration:generate src/infrastructure/database/migrations/tenanted/$npm_config_name",
    "migrations:tenanted:create": "npm run typeorm-tenanted migration:create src/infrastructure/database/migrations/tenanted/$npm_config_name",
    "migrations:tenanted:run": "npm run typeorm-tenanted migration:run",
    "migrations:tenanted:show": "npm run typeorm-tenanted migration:show"
  },
  "dependencies": {
    "@apollo/server": "^4.7.3",
    "@aws-sdk/client-s3": "^3.360.0",
    "@casl/ability": "^6.5.0",
    "@nestjs/apollo": "^11.0.6",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.3",
    "@nestjs/core": "^9.0.0",
    "@nestjs/graphql": "^11.0.6",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/typeorm": "^9.0.1",
    "@squareboat/nest-mailman": "^1.0.5",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "date-fns": "^2.30.0",
    "dotenv": "^16.1.4",
    "graphql": "^16.6.0",
    "graphql-type-json": "^0.3.2",
    "graphql-upload-minimal": "^1.5.4",
    "joi": "^17.9.2",
    "jwt-decode": "^3.1.2",
    "limax": "^4.1.0",
    "luxon": "^3.3.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.0",
    "react-dom": "^18.2.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "typeorm": "^0.3.16",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/node": "18.16.12",
    "@types/pg": "^8.10.2",
    "@types/react": "^18.2.14",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-plugin-prettier": "^4.0.0",
    "source-map-support": "^0.5.20",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
